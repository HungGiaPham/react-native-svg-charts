{
    "name": "react-native-svg-charts",
    "version": "5.5.0",
    "private": false,
    "description": "Customizable charts (Line, Bar, Area, Pie, Circle, Progress) for React Native-Upgrade for react-native 0.67.3",
    "main": "lib/module/index.js",
    "module": "lib/module/index.js",
    "react-native": "src/index.js",
    "scripts": {
        "start": "node node_modules/react-native/local-cli/cli.js start",
        "test": "jest",
        "storybook": "storybook start -p 7008",
        "eslint": "./node_modules/.bin/eslint .",
        "prepare": "bob build"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "lint-staged": {
        "*.js": "eslint"
    },
    "keywords": [
        "react-native",
        "react-component",
        "react-native-component",
        "react",
        "mobile",
        "ios",
        "android",
        "vector",
        "charts",
        "chart",
        "d3"
    ],
    "author": {
        "name": "Jesper Lekland",
        "email": "jesper.lekland@push2prod.se"
    },
    "homepage": "https://github.com/JesperLekland/react-native-svg-charts",
    "bugs": {
        "url": "https://github.com/JesperLekland/react-native-svg-charts/issues"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/JesperLekland/react-native-svg-charts.git"
    },
    "license": "MIT",
    "dependencies": {
        "d3-array": "^1.2.0",
        "d3-interpolate-path": "2.0.0",
        "d3-scale": "^1.0.6",
        "d3-shape": "^1.0.6",
        "prop-types": "^15.6.0"
    },
    "peerDependencies": {
        "react": ">=17.0.2",
        "react-native": ">=0.67.3",
        "react-native-svg": ">=14.1.0"
    },
    "devDependencies": {
        "@react-native-community/bob": "^0.10.0",
        "@storybook/addon-actions": "^3.2.14",
        "@storybook/addon-knobs": "^3.2.14",
        "@storybook/addon-links": "^3.2.14",
        "@storybook/react-native": "^3.2.14",
        "babel-eslint": "^7.2.3",
        "babel-jest": "26.6.3",
        "babel-preset-react-native": "1.9.1",
        "date-fns": "^1.28.5",
        "eslint": "8.0.1",
        "eslint-config-standard": "17.1.0",
        "eslint-plugin-import": "2.29.1",
        "eslint-plugin-node": "11.1.0",
        "eslint-plugin-promise": "6.1.1",
        "eslint-plugin-react": "7.33.2",
        "eslint-plugin-react-native": "4.1.0",
        "eslint-plugin-standard": "5.0.0",
        "husky": "^3.0.0",
        "jest": "20.0.1",
        "lint-staged": "^7.1.3",
        "prettier": "^1.18.2",
        "pretty-quick": "^1.11.1",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-native": "0.67.3",
        "react-native-svg": "14.1.0",
        "react-test-renderer": "17.0.2"
    },
    "jest": {
        "preset": "react-native",
        "modulePathIgnorePatterns": [
            "<rootDir>/lib/"
        ]
    },
    "files": [
        "src",
        "lib"
    ],
    "@react-native-community/bob": {
        "source": "src",
        "output": "lib",
        "targets": [
            "module"
        ]
    },
    "eslintIgnore": [
        "node_modules/",
        "lib/"
    ]
}
